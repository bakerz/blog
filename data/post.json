---
---
{
	"datas": [
{% for post in site.posts %}
	{
		"id": "{{ post.id }}",
		"title": "{{ post.title }}",
		"url": "{{ site.url }}{{ post.url }}",
		"date": "{{ post.date | date: '%Y-%m-%d' }}",
		"category": "{{ post.category }}",
		"tags": [
			{% for tag in post.tags %}"{{ tag }}"{% if forloop.last == false %},{% endif %}{% endfor %}
		]
	}{% if forloop.last == false %},{% endif %}
{% endfor %}
	],
	"size": {{ site.posts | size }},
	"category": [],
	"tags": []
}